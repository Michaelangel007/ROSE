{
"update_url": "https://clients2.google.com/service/update2/crx",

  "file_handlers": {
    "text": {
      "types": [
        "application/javascript",
        "application/json",
        "application/x-shellscript",
        "application/xml",
        "text/*"
      ],
      "extensions": [
        "bin",
        "gb"
      ]
    }
  },
  "offline_enabled": true,
  "permissions": [
    "app.window.alwaysOnTop",
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "storage",
    "https://www.google-analytics.com/"
  ],
  "name": "ROSE",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "version": "0.5"
}